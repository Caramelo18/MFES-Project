class Profile
	types
		public String = seq of char;
	instance variables
	  protected name: String;
	
	operations	
		public Profile: String ==> Profile
		Profile(n) == (
			name:= n;
			return self;
		);
	  public  changeName : String  ==> ()
	  changeName(nn) == (
	  	name := nn;
	  )
	  pre name <> nn
	  post name~ = nn;
	  
	  public getName: () ==> String
	  getName() == (
	  	return name;
	  )
	  post RESULT = name;

end Profile
