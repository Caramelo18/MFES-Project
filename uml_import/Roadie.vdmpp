class Roadie is subclass of Profile
	instance variables
	  task: JyveUtils`Task;
	
	operations
		public Roadie : String * JyveUtils`Task ==> Roadie
		Roadie(n, t) == (
			name:= n;
			task:= t;
			return self;
		);
		
	  public setTask : JyveUtils`Task  ==> ()
	  setTask(nt) ==(
	  	task := nt;
	  )
	  pre task <> nt
	  post task = nt;
	  
	  public getTask: () ==> JyveUtils`Task
	  getTask() == (
	  	return task;
	  )
	  post task = task~;
	  
	  

end Roadie
