class Roadie is subclass of Profile
	instance variables
	  task: Utilitys`Task;
	
	operations
		public Roadie : Utilitys`String * Utilitys`Task ==> Roadie
		Roadie(n, t) == (
			name:= n;
			task:= t;
			return self;
		);
		
	  public  setNewTask : Utilitys`Task  ==> ()
	  setNewTask(nt) ==(
	  	task := nt;
	  )
	  pre task <> nt
	  post task~ = nt;

end Roadie
