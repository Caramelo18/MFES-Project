class Roadie is subclass of Profile
	instance variables
	  task: Utils`String;
	
	operations
		public Roadie : Utils`String * Utils`String ==> Roadie
		Roadie(n, t) == (
			name:= n;
			task:= t;
			return self;
		);
		
	  public  setNewTask : Utils`String  ==> ()
	  setNewTask(nt) ==(
	  	task := nt;
	  )
	  pre task <> nt
	  post task~ = nt;
	

end Roadie
