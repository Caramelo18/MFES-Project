class Place
	types 
		public String :: seq of (char);
	
	instance variables
	  placeOwner: PlaceOwner;
	  location: String;
	  stage_area: int;
	  inv stage_area > 0;
	  
	operations
	  public Place: PlaceOwner * String * int ==> Place 
	  Place(po, l, sa) == (
	  	placeOwner := po;
	  	location := l;
	  	stage_area := sa;
	  );
	  
	  public changeStage_area : int ==> ()
		changeStage_area(nsa) == (
			stage_area := nsa;
		)
		pre stage_area <> nsa
		post stage_area~ = nsa;

end Place
