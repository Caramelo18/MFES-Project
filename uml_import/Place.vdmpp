class Place
	instance variables
	  placeOwner: PlaceOwner;
	  location: Utils`String;
	  stage_area: nat1;
	  inv stage_area > 0;
	  
	operations
	  public Place: PlaceOwner * Utils`String * nat1 ==> Place 
	  Place(po, l, sa) == (
	  	placeOwner := po;
	  	location := l;
	  	stage_area := sa;
	  	return self;
	  );
	  
	  public changeStage_area : nat1 ==> ()
		changeStage_area(nsa) == (
			stage_area := nsa;
		)
		pre stage_area <> nsa
		post stage_area~ = nsa;

end Place
