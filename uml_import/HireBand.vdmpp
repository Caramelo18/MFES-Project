class HireBand is subclass of Offer
	instance variables
	  eventOrganizer: EventOrganizer;
	  location: seq of char;
	  date: JyveUtils`Date;
	
	operations
		public HireBand : EventOrganizer * seq of char * JyveUtils`Date ==> HireBand
		HireBand(e, l, d) == (
			eventOrganizer := e;
			location := l;
			date := d;
			return self;
		);

end HireBand
