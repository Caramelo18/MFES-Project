class HireBand is subclass of Offer
	types
	instance variables
	  eventOrganizer: EventOrganizer;
	  location: Utilitys`String;
	  date: Utilitys`Date;
	
	operations
		public HireBand : EventOrganizer * Utilitys`String * Utilitys`Date ==> HireBand
		HireBand(e, l, d) == (
			eventOrganizer := e;
			location := l;
			date := d;
			return self;
		);

end HireBand
