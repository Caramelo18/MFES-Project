class HireBand is subclass of Offer
	types
		public String :: seq of char;
	instance variables
	  eventOrganizer: EventOrganizer;
	  location: seq of char;
	  date: Utilitys`Date;
	
	operations
		public HireBand : EventOrganizer * seq of char * Utilitys`Date ==> HireBand
		HireBand(e, l, d) == (
			eventOrganizer := e;
			location := l;
			date := d;
			return self;
		);

end HireBand
