class HireBand is subclass of Offer
	types
	instance variables
	  eventOrganizer: EventOrganizer;
	  location: Utils`String;
	  date: Utils`Date;
	
	operations
		public HireBand : EventOrganizer * Utils`String * Utils`Date ==> HireBand
		HireBand(e, l, d) == (
			eventOrganizer := e;
			location := l;
			date := d;
			return self;
		);

end HireBand
