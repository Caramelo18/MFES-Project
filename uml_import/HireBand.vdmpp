class HireBand is subclass of Offer
	types
		public String :: seq of (char);
		public Date :: year : nat
									 month: nat
									 day  : nat;
	instance variables
	  eventOrganizer: EventOrganizer;
	  location: String;
	  date: Date;
	
	operations
		public HireBand : EventOrganizer * String * Date ==> HireBand
		HireBand(e, l, d) == (
			eventOrganizer := e;
			location := l;
			date := d;
		);

end HireBand
