class PlaceOffer is subclass of Offer
	instance variables
	  place: Place;
	
	operations
	  public  PlaceOffer : Place  ==> PlaceOffer
	  PlaceOffer(rp) == (
	  	place := rp;
	  	return self;
	  );
	  
	  public getPlace: () ==> Place
	  getPlace() == (
	  	return place
	  )
	  post place = place~
	  		and RESULT = place;

end PlaceOffer
