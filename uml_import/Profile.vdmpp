class Profile
	types
		public String :: seq of (char);
		
	instance variables
	  name: String;
	
	operations
		public Profile : String ==> Profile
		Profile(n) == (
			name := n;
		);
	
	  public  changeName : String  ==> ()
	  changeName(nn) == (
	  	name := nn;
	  )
	  pre name <> nn
	  post name~ = nn;

end Profile
