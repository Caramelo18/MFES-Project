class Profile
	instance variables
	  protected name: seq of char;
	
	operations	
		public Profile: seq of char ==> Profile
		Profile(n) == (
			name:= n;
			return self;
		);
	  public  changeName : seq of char  ==> ()
	  changeName(nn) == (
	  	name := nn;
	  )
	  pre name <> nn
	  post name = nn;
	  
	  public getName: () ==> seq of char
	  getName() == (
	  	return name;
	  )
	  post RESULT = name;

end Profile
